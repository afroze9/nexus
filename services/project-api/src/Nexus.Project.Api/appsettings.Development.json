{
  "FrameworkSettings": {
    "Auth": {
      "Enable": true,
      "ResourceName": "project"
    },
    "Swagger": {
      "Enable": true,
      "Version": "v1",
      "Title": "Project API",
      "Description": "Project API"
    },
    "ApiControllers": {
      "Enable": true,
      "Filters": {
        "EnableActionLogging": true
      }
    },
    "Telemetry": {
      "Enable": true,
      "Endpoint": "http://localhost:4317",
      "ServiceName": "project-api",
      "ServiceVersion": "v1",
      "EnableAlwaysOnSampler": false,
      "SampleProbability": 0.2,
      "EnableConsoleExporter": false
    },
    "Management": {
      "Enable": true
    },
    "Discovery": {
      "Enable": true
    },
    "Logging": {
      "Enable": true
    }
  }
}
