    "Audience": "projectmanagement"
  },
  "SerilogSettings": {
    "ElasticSearchSettings": {
      "Uri": "https://localhost:9200",
      "Username": "elastic",
      "Password": "dev123",
      "IndexFormat": "logs-company-api-{0:yyyy.MM}"
    }
  },
  "Serilog": {
    "MinimumLevel": {
